    <dependencies>
    
    	<!-- Llibreries degloba -->
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>boundedContext1-domain</artifactId>
            <version>${project.version}</version>
        </dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>degloba-domain</artifactId>
			<version>${project.version}</version>
		</dependency>	
		
		<dependency>
