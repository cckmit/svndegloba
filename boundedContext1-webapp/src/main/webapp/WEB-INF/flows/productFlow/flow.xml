<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow 
	http://www.springframework.org/schema/webflow/spring-webflow-2.4.xsd">

	<!--
		Also see ../parent-flow.xml. 
	-->

	<var name="rentYourStuffView" class="com.degloba.rent.ui.webui.spring.RentYourStuffView" />
	
	<view-state id="view" >
        <transition on="addProductAction"  >
            <evaluate expression="rentYourStuffView.onAddProduct(flowRequestContext)" />
		</transition>
    </view-state>
		
</flow>
